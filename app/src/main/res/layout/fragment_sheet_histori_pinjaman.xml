<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cons_fr_sheet_his_pinjaman"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="24dp"
    tools:context=".view.sheet.DetailPeminjamSheetFragment">

    <ImageView
        android:id="@+id/img_handle_fr_sheet_his_pinjaman"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:contentDescription="@string/img"
        android:src="@drawable/ic_handle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lbl_his_pinjaman_fr_sheet_his_pinjaman"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/ibm_plex_sans_medium"
        android:text="@string/histori_peminjam"
        android:textColor="@color/dark"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_handle_fr_sheet_his_pinjaman" />

    <TextView
        android:id="@+id/lbl_one_fr_sheet_his_pinjaman"
        android:layout_width="176dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="@string/frek_byr_tepat"
        android:textColor="@color/darkO05"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/lbl_his_pinjaman_fr_sheet_his_pinjaman"
        app:layout_constraintTop_toBottomOf="@+id/lbl_his_pinjaman_fr_sheet_his_pinjaman" />

    <TextView
        android:id="@+id/lbl_two_fr_sheet_his_pinjaman"
        android:layout_width="176dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/frek_byr_late"
        android:textColor="@color/darkO05"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/lbl_one_fr_sheet_his_pinjaman"
        app:layout_constraintTop_toBottomOf="@+id/lbl_one_fr_sheet_his_pinjaman" />

    <TextView
        android:id="@+id/lbl_three_fr_sheet_his_pinjaman"
        android:layout_width="176dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/tot_pin_galang"
        android:textColor="@color/darkO05"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/lbl_two_fr_sheet_his_pinjaman"
        app:layout_constraintTop_toBottomOf="@+id/lbl_two_fr_sheet_his_pinjaman" />

    <TextView
        android:id="@+id/lbl_four_fr_sheet_his_pinjaman"
        android:layout_width="176dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/tot_pin_aktif"
        android:textColor="@color/darkO05"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/lbl_three_fr_sheet_his_pinjaman"
        app:layout_constraintTop_toBottomOf="@+id/lbl_three_fr_sheet_his_pinjaman" />

    <TextView
        android:id="@+id/txt_frek_byr_tepat_fr_sheet_his_pinjaman"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:maxLines="1"
        android:text="@string/txtnull"
        android:textAlignment="textEnd"
        android:textColor="@color/dark"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lbl_one_fr_sheet_his_pinjaman"
        app:layout_constraintTop_toTopOf="@+id/lbl_one_fr_sheet_his_pinjaman" />

    <TextView
        android:id="@+id/txt_frek_bayar_telat_fr_sheet_his_pinjaman"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:maxLines="1"
        android:text="@string/txtnull"
        android:textAlignment="textEnd"
        android:textColor="@color/dark"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="@+id/txt_frek_byr_tepat_fr_sheet_his_pinjaman"
        app:layout_constraintStart_toEndOf="@+id/lbl_two_fr_sheet_his_pinjaman"
        app:layout_constraintTop_toTopOf="@+id/lbl_two_fr_sheet_his_pinjaman" />

    <TextView
        android:id="@+id/txt_tot_pin_galang_fr_sheet_his_pinjaman"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:maxLines="1"
        android:text="@string/txtnull"
        android:textAlignment="textEnd"
        android:textColor="@color/dark"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="@+id/txt_frek_bayar_telat_fr_sheet_his_pinjaman"
        app:layout_constraintStart_toEndOf="@+id/lbl_three_fr_sheet_his_pinjaman"
        app:layout_constraintTop_toTopOf="@+id/lbl_three_fr_sheet_his_pinjaman" />

    <TextView
        android:id="@+id/lbl_five_fr_sheet_his_pinjaman"
        android:layout_width="176dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/tot_pin_selesai"
        android:textColor="@color/darkO05"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/lbl_four_fr_sheet_his_pinjaman"
        app:layout_constraintTop_toBottomOf="@+id/lbl_four_fr_sheet_his_pinjaman" />

    <TextView
        android:id="@+id/txt_tot_pin_aktif_fr_sheet_his_pinjaman"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:maxLines="1"
        android:text="@string/txtnull"
        android:textAlignment="textEnd"
        android:textColor="@color/dark"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lbl_four_fr_sheet_his_pinjaman"
        app:layout_constraintTop_toTopOf="@+id/lbl_four_fr_sheet_his_pinjaman" />

    <TextView
        android:id="@+id/txt_tot_pin_selesai_fr_sheet_his_pinjaman"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:maxLines="1"
        android:text="@string/txtnull"
        android:textAlignment="textEnd"
        android:textColor="@color/dark"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/lbl_five_fr_sheet_his_pinjaman"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lbl_five_fr_sheet_his_pinjaman"
        app:layout_constraintTop_toTopOf="@+id/lbl_five_fr_sheet_his_pinjaman" />

</androidx.constraintlayout.widget.ConstraintLayout>