                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="16dp"
                android:fontFamily="@font/ibm_plex_sans_medium"
                android:text="@string/address_based_operasional"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/edit_akta_kodepos_fr_com_address" />

            <CheckBox
                android:id="@+id/cb_operasional_same_as_akta_fr_com_address"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="16dp"
                android:button="@drawable/checkbox_selector"
                app:useMaterialThemeColors="false"
                android:fontFamily="@font/ibm_plex_sans"
                android:maxLines="1"
                android:paddingStart="8dp"
                android:paddingLeft="8dp"
                android:paddingBottom="2dp"
                android:text="@string/address_same_as_akta"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lbl_on_operasional_fr_com_address" />


